<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/nature"
    tools:context="com.smartbrain.giovanny.smartbrain.MenuMediumActivity"
    >


    <ImageView
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:id="@+id/cloud"
        android:background="@drawable/cloud"
        android:layout_below="@+id/cloud1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="56dp"
        android:layout_marginEnd="56dp" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:id="@+id/cloud1"
        android:background="@drawable/cloud1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:id="@+id/cloud2"
        android:background="@drawable/cloud2"
        android:layout_marginTop="84dp"
        android:layout_below="@+id/cloud"
        android:layout_toLeftOf="@+id/cloud"
        android:layout_toStartOf="@+id/cloud" />

    <ImageView
        android:layout_width="50"
        android:layout_height="140"
        android:id="@+id/flower1"
        android:background="@drawable/flower1"
        android:layout_above="@+id/flower3"
        android:layout_alignRight="@+id/cloud2"
        android:layout_alignEnd="@+id/cloud2" />

    <ImageView
        android:layout_width="50"
        android:layout_height="140"
        android:id="@+id/flower2"
        android:background="@drawable/flower2"
        android:layout_marginLeft="82dp"
        android:layout_marginStart="82dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/smoke"
        android:layout_toEndOf="@+id/smoke" />

    <ImageView
        android:layout_width="50"
        android:layout_height="140"
        android:id="@+id/flower3"
        android:background="@drawable/flower3"
        android:layout_above="@+id/flower2"
        android:layout_toRightOf="@+id/flower2"
        android:layout_toEndOf="@+id/flower2"
        android:layout_marginLeft="92dp"
        android:layout_marginStart="92dp"
        android:layout_marginBottom="82dp" />

    <ImageView
        android:layout_width="60"
        android:layout_height="60"
        android:id="@+id/smoke"
        android:background="@drawable/smoke"
        android:layout_above="@+id/flower1"
        android:layout_toLeftOf="@+id/cloud1"
        android:layout_toStartOf="@+id/cloud1" />



</RelativeLayout>