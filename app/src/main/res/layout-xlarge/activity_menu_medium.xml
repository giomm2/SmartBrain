<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/nature"
    tools:context="com.smartbrain.giovanny.smartbrain.MenuMediumActivity"
    >


    <ImageView
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:id="@+id/cloud"
        android:background="@drawable/cloud"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="90dp"
        android:id="@+id/flower1"
        android:background="@drawable/flower1"
        android:layout_marginRight="51dp"
        android:layout_marginEnd="51dp"
        android:layout_alignBottom="@+id/flower3"
        android:layout_toLeftOf="@+id/cloud"
        android:layout_toStartOf="@+id/cloud"
        android:layout_marginBottom="39dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="90dp"
        android:id="@+id/flower2"
        android:background="@drawable/flower2"
        android:layout_marginLeft="168dp"
        android:layout_marginStart="168dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="90dp"
        android:id="@+id/flower3"
        android:background="@drawable/flower3"
        android:layout_marginLeft="94dp"
        android:layout_marginStart="94dp"
        android:layout_above="@+id/flower2"
        android:layout_toRightOf="@+id/flower2"
        android:layout_toEndOf="@+id/flower2"
        android:layout_marginBottom="33dp" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:id="@+id/smoke"
        android:background="@drawable/smoke"
        android:layout_marginTop="45dp"
        android:layout_alignTop="@+id/cloud"
        android:layout_alignRight="@+id/flower1"
        android:layout_alignEnd="@+id/flower1" />



</RelativeLayout>