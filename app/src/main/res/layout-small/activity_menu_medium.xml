<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/nature"
    tools:context="com.smartbrain.giovanny.smartbrain.MenuMediumActivity"
    >


    <ImageView
        android:layout_width="70dp"
        android:layout_height="65dp"
        android:id="@+id/cloud"
        android:background="@drawable/cloud"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:id="@+id/flower1"
        android:background="@drawable/flower1"
        android:layout_marginRight="62dp"
        android:layout_marginEnd="62dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/flower3"
        android:layout_toStartOf="@+id/flower3"
        android:layout_marginBottom="45dp" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:id="@+id/flower2"
        android:background="@drawable/flower2"
        android:layout_alignTop="@+id/flower3"
        android:layout_toLeftOf="@+id/cloud"
        android:layout_toStartOf="@+id/cloud" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:id="@+id/flower3"
        android:background="@drawable/flower3"
        android:layout_alignTop="@+id/flower1"
        android:layout_toLeftOf="@+id/smoke"
        android:layout_toStartOf="@+id/smoke"
        android:layout_marginRight="27dp"
        android:layout_marginEnd="27dp" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/smoke"
        android:background="@drawable/smoke"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/cloud"
        android:layout_toStartOf="@+id/cloud" />

</RelativeLayout>