<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/nature"
    tools:context="com.smartbrain.giovanny.smartbrain.MenuMediumActivity"
    >


    <ImageView
        android:layout_width="70dp"
        android:layout_height="65dp"
        android:id="@+id/cloud"
        android:background="@drawable/cloud"
        android:layout_marginLeft="142dp"
        android:layout_marginStart="142dp"
        android:clickable="false"
        android:layout_below="@+id/smoke"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/flower1"
        android:background="@drawable/flower1"
        android:layout_alignTop="@+id/flower3"
        android:layout_toLeftOf="@+id/flower3"
        android:layout_toStartOf="@+id/flower3" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/flower2"
        android:background="@drawable/flower2"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_alignTop="@+id/flower3"
        android:layout_toRightOf="@+id/flower3"
        android:layout_toEndOf="@+id/flower3" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/flower3"
        android:background="@drawable/flower3"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/cloud"
        android:layout_toEndOf="@+id/cloud"
        android:layout_marginBottom="31dp" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="60dp"
        android:id="@+id/smoke"
        android:background="@drawable/smoke"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/flower2"
        android:layout_alignEnd="@+id/flower2" />

</RelativeLayout>